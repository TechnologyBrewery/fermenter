<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE targets PUBLIC "-//FERMENTER//DTD TARGETS//EN" "https://bitbucket.org/askllc/fermenter/downloads/targets.dtd">

<targets>

	<target name="objective-c-entity-header"
			generator="org.tigris.atlas.mda.generator.entity.ObjectiveCEntityGenerator"
			templateName="templates/entity.header.objective.c.vm"
			outputFile="${entityName}.h"
			overwritable="true"/>

	<target name="objective-c-entity-source"
			generator="org.tigris.atlas.mda.generator.entity.ObjectiveCEntityGenerator"
			templateName="templates/entity.source.objective.c.vm"
			outputFile="${entityName}.m"
			overwritable="true"/>

	<target name="objective-c-enumeration-header"
			generator="org.tigris.atlas.mda.generator.enumeration.ObjectiveCEnumerationGenerator"
			templateName="templates/enumeration.header.objective.c.vm"
			outputFile="${enumerationName}.h"
			overwritable="true"/>

	<target name="objective-c-enumeration-source"
			generator="org.tigris.atlas.mda.generator.enumeration.ObjectiveCEnumerationGenerator"
			templateName="templates/enumeration.source.objective.c.vm"
			outputFile="${enumerationName}.m"
			overwritable="true"/>

	<target name="objective-c-service-header"
			generator="org.tigris.atlas.mda.generator.service.ObjectiveCServiceGenerator"
			templateName="templates/service.header.objective.c.vm"
			outputFile="${serviceName}Service.h"
			overwritable="true"/>

	<target name="objective-c-service-source"
			generator="org.tigris.atlas.mda.generator.service.ObjectiveCServiceGenerator"
			templateName="templates/service.source.objective.c.vm"
			outputFile="${serviceName}Service.m"
			overwritable="true"/>

	<target name="objective-c-service-response-header"
			generator="org.tigris.atlas.mda.generator.service.ObjectiveCServiceResponseGenerator"
			templateName="templates/service.response.header.objective.c.vm"
			outputFile="${operationName}Response.h"
			overwritable="true"/>

	<target name="objective-c-service-response-source"
			generator="org.tigris.atlas.mda.generator.service.ObjectiveCServiceResponseGenerator"
			templateName="templates/service.response.source.objective.c.vm"
			outputFile="${operationName}Response.m"
			overwritable="true"/>

</targets>