<?xml version="1.0"?>
<!DOCTYPE entity PUBLIC "-//FERMENTER//DTD ENTITY//EN" "https://bitbucket.org/cpointe/fermenter/downloads/fermenter-2-entity.dtd">

<entity>
	<name>GenerationTestEntity</name>
	<documentation>Entity to help with some generation edge cases</documentation>
	<table>GEN_TEST_ENTITY</table>

	<id>
		<!-- every other entity happens to use id for the id field name, which 
			makes it easy to make generation mistakes that don't get caught until someone 
			else picks up a release: -->
		<field>
			<name>primaryKey</name>
			<type>uuid</type>
			<column>GEN_TEST_ENTITY_ID</column>
			<required>true</required>
			<generator>identity</generator>
		</field>
	</id>

</entity>