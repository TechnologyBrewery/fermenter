<?xml version="1.0"?>
<!DOCTYPE entity PUBLIC "-//FERMENTER//DTD ENTITY//EN" "https://bitbucket.org/cpointe/fermenter/downloads/fermenter-2-entity-transient.dtd">

<entity>
	<name>TransientFieldExample</name>
	<namespace>org.bitbucket.askllc.fermenter.cookbook.domain</namespace>
	<documentation>Entity that demonstrates the use of a transient field</documentation>
	<table>TRANSIENT_FIELD_ENTITY</table>

	<id>
		<field>
			<name>primaryKey</name>
			<type>uuid</type>
			<column>TRANSIENT_FIELD_ID</column>
			<required>true</required>
			<generator>uuid</generator>
		</field>
	</id>

	<fields>
		<field>
			<name>beerType</name>
			<documentation>What kind of beer this is</documentation>
			<type>string</type>
			<column>BEER_TYPE</column>
		</field>
		<field>
			<name>transientField</name>
			<documentation>Testing transient field property</documentation>
			<type>string</type>
			<column>NONE_NEEDED</column>
			<transient>true</transient>
		</field>
	</fields>

</entity>