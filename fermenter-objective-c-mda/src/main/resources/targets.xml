<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE targets PUBLIC "-//FERMENTER//DTD TARGETS//EN" "https://bitbucket.org/askllc/fermenter/downloads/targets.dtd">

<targets>

	<target name="objective-c-interface"
			generator="org.tigris.atlas.mda.generator.entity.ObjectiveCEntityGenerator"
			templateName="templates/interface.objective.c.vm"
			outputFile="${entityName}.h"
			overwritable="true"/>

	<target name="objective-c-implementation"
			generator="org.tigris.atlas.mda.generator.entity.ObjectiveCEntityGenerator"
			templateName="templates/implementation.objective.c.vm"
			outputFile="${entityName}.m"
			overwritable="true"/>

	<target name="objective-c-enumeration"
			generator="org.tigris.atlas.mda.generator.enumeration.ObjectiveCEnumerationGenerator"
			templateName="templates/enumeration.objective.c.vm"
			outputFile="${enumerationName}.h"
			overwritable="true"/>

</targets>