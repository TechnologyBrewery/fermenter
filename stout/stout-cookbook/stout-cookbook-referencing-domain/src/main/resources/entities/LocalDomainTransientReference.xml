<?xml version="1.0"?>
<!DOCTYPE entity PUBLIC "-//FERMENTER//DTD ENTITY//EN" "https://bitbucket.org/cpointe/fermenter/downloads/fermenter-2-entity.dtd">

<entity>
	<name>LocalDomainTransientReference</name>
	<namespace>org.bitbucket.askllc.fermenter.cookbook.referencing.domain</namespace>
	<documentation>A local persistent domain with external reference to transient entity</documentation>
	<table>LOCAL_DOMAIN_TWO</table>

	<id>
		<field>
			<name>id</name>
			<type>uuid</type>
			<column>LOCAL_DOMAIN_TWO_ID</column>
			<required>true</required>
			<generator>uuid</generator>
		</field>
	</id>

	<fields>
		<field>
			<name>name</name>
			<documentation>The name of the entity</documentation>
			<type>string</type>
			<column>NAME</column>
		</field>
	</fields>

	<associations>
		<reference>
			<name>externalTransientReference</name>
			<type>ValidationTransientReferencedObject</type>
			<required>true</required>
		</reference>

	</associations>

</entity>