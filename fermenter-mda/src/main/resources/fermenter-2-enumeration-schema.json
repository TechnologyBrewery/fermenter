{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"id": "#enumeration",
	"title": "Enumeration",
	"type": "object",
	"additionalProperties": false,
	"properties": {
		"name": {
			"type": "string"
		},
		"package": {
			"type": "string"
		},
		"enums": {
			"type": "array",
			"items": {
				"type": "object",
				"properties": {
					"name": {
						"type": "string"
					},
					"value": {
						"type": "string"
					}
				}
			},
			"required": [
				"name"
			]
		}
	},
	"required": [
		"name", "package"
	]
}