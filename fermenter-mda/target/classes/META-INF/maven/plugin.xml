<?xml version="1.0" encoding="UTF-8"?>
<plugin>
  <name>Fermenter Model-Driven Architecture Code Generator</name>
  <description></description>
  <groupId>com.ask.fermenter</groupId>
  <artifactId>fermenter-mda</artifactId>
  <version>1-SNAPSHOT</version>
  <goalPrefix>fermenter-mda</goalPrefix>
  <isolatedRealm>false</isolatedRealm>
  <inheritedByDefault>true</inheritedByDefault>
  <mojos>
    <mojo>
      <goal>clean</goal>
      <description></description>
      <requiresDirectInvocation>false</requiresDirectInvocation>
      <requiresProject>true</requiresProject>
      <requiresReports>false</requiresReports>
      <aggregator>false</aggregator>
      <requiresOnline>false</requiresOnline>
      <inheritedByDefault>true</inheritedByDefault>
      <implementation>org.tigris.atlas.mda.CleanMojo</implementation>
      <language>java</language>
      <instantiationStrategy>per-lookup</instantiationStrategy>
      <executionStrategy>once-per-session</executionStrategy>
      <threadSafe>false</threadSafe>
      <parameters>
        <parameter>
          <name>generatedBasedir</name>
          <type>java.io.File</type>
          <required>true</required>
          <editable>true</editable>
          <description>Base Java source directory.  All generated Java source code will wind
up underneath this folder.  This folder will be cleaned as part of
the clean target, and its contents should not be put under version
control.</description>
        </parameter>
        <parameter>
          <name>xmlBeansBasedir</name>
          <type>java.io.File</type>
          <required>true</required>
          <editable>true</editable>
          <description></description>
        </parameter>
      </parameters>
      <configuration>
        <generatedBasedir implementation="java.io.File">${project.basedir}/src/generated</generatedBasedir>
        <xmlBeansBasedir implementation="java.io.File">${project.basedir}/src/xmlbeans</xmlBeansBasedir>
      </configuration>
    </mojo>
    <mojo>
      <goal>generate-sources</goal>
      <description></description>
      <requiresDependencyResolution>compile</requiresDependencyResolution>
      <requiresDirectInvocation>false</requiresDirectInvocation>
      <requiresProject>true</requiresProject>
      <requiresReports>false</requiresReports>
      <aggregator>false</aggregator>
      <requiresOnline>false</requiresOnline>
      <inheritedByDefault>true</inheritedByDefault>
      <phase>generate-sources</phase>
      <implementation>org.tigris.atlas.mda.GenerateSourcesMojo</implementation>
      <language>java</language>
      <instantiationStrategy>per-lookup</instantiationStrategy>
      <executionStrategy>once-per-session</executionStrategy>
      <threadSafe>false</threadSafe>
      <parameters>
        <parameter>
          <name>basePackage</name>
          <type>java.lang.String</type>
          <required>true</required>
          <editable>true</editable>
          <description></description>
        </parameter>
        <parameter>
          <name>generatedCompileSourceRoot</name>
          <type>java.lang.String</type>
          <required>true</required>
          <editable>false</editable>
          <description></description>
        </parameter>
        <parameter>
          <name>generatedSourceRoot</name>
          <type>java.io.File</type>
          <required>true</required>
          <editable>false</editable>
          <description></description>
        </parameter>
        <parameter>
          <name>mainSourceRoot</name>
          <type>java.io.File</type>
          <required>true</required>
          <editable>false</editable>
          <description></description>
        </parameter>
        <parameter>
          <name>metadataDependencies</name>
          <type>java.util.List</type>
          <required>false</required>
          <editable>true</editable>
          <description></description>
        </parameter>
        <parameter>
          <name>profile</name>
          <type>java.lang.String</type>
          <required>true</required>
          <editable>true</editable>
          <description></description>
        </parameter>
        <parameter>
          <name>project</name>
          <type>org.apache.maven.project.MavenProject</type>
          <required>true</required>
          <editable>false</editable>
          <description></description>
        </parameter>
        <parameter>
          <name>xmlBeansSourceRoot</name>
          <type>java.lang.String</type>
          <required>true</required>
          <editable>false</editable>
          <description></description>
        </parameter>
      </parameters>
      <configuration>
        <generatedCompileSourceRoot implementation="java.lang.String">${project.basedir}/src/generated/java</generatedCompileSourceRoot>
        <generatedSourceRoot implementation="java.io.File">${project.basedir}/src/generated</generatedSourceRoot>
        <mainSourceRoot implementation="java.io.File">${project.basedir}/src/main</mainSourceRoot>
        <project implementation="org.apache.maven.project.MavenProject">${project}</project>
        <xmlBeansSourceRoot implementation="java.lang.String">${project.basedir}/src/xmlbeans/java</xmlBeansSourceRoot>
      </configuration>
    </mojo>
  </mojos>
  <dependencies>
