<?xml version="1.0"?>
<!DOCTYPE entity PUBLIC "-//FERMENTER//DTD ENTITY//EN" "https://bitbucket.org/cpointe/fermenter/downloads/fermenter-2-entity.dtd">

<entity>
	<name>SimpleDomain</name>
	<namespace>org.bitbucket.askllc.fermenter.cookbook.domain</namespace>
	<documentation>A simple domain entity with a generated id field, fields of different types, and a 1-M relation</documentation>
	<table>SIMPLE_DOMAIN</table>

	<id>
		<field>
			<name>id</name>
			<type>uuid</type>
			<column>SIMPLE_DOMAIN_ID</column>
			<required>true</required>
			<generator>uuid</generator>
		</field>
	</id>

	<fields>
		<field>
			<name>name</name>
			<documentation>The friendly domain moniker</documentation>
			<type>string</type>
			<column>NAME</column>
		</field>
		<field>
			<name>type</name>
			<documentation>A simple domain classification</documentation>
			<type>string</type>
			<column>TYPE</column>
		</field>
		<field>
			<name>theDate1</name>
			<documentation>An important date for this domain</documentation>
			<type>date</type>
			<column>DATE1</column>
		</field>
		<field>
			<name>theLong1</name>
			<documentation>A relevant number for this domain</documentation>
			<type>long</type>
			<column>LONG1</column>
		</field>
		<field>
			<name>anEnumeratedValue</name>
			<type>SimpleDomainEnumeration</type>
			<column>SIMPLE_DOMAIN_ENUM</column>
		</field>
		<field>
			<name>bigDecimalValue</name>
			<type>big_decimal</type>
			<column>BIG_DECIMAL_COLUMN</column>
		</field>
		<field>
			<name>standardBoolean</name>
			<type>boolean</type>
			<column>STANDARD_BOOLEAN</column>
		</field>
		<field>
			<name>numericBoolean</name>
			<type>numeric_boolean</type>
			<column>NUMERIC_BOOLEAN</column>
		</field>
	</fields>

	<associations>
		<relation>
			<type>SimpleDomainChild</type>
			<multiplicity>1-M</multiplicity>
			<fetchMode>lazy</fetchMode>
		</relation>
		<relation>
			<type>SimpleDomainEagerChild</type>
			<multiplicity>1-M</multiplicity>
		</relation>
	</associations>

</entity>